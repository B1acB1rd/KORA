{
  "name": "kora-rent-reclaimer",
  "version": "1.0.0",
  "description": "Automated rent reclaim bot for Kora-sponsored Solana accounts",
  "main": "dist/index.js",
  "bin": {
    "kora-reclaimer": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "build:binary": "npm run build && pkg . --out-path ./bin",
    "build:windows": "npm run build && pkg . --target node18-win-x64 --output ./bin/kora-reclaimer.exe",
    "build:linux": "npm run build && pkg . --target node18-linux-x64 --output ./bin/kora-reclaimer-linux",
    "build:macos": "npm run build && pkg . --target node18-macos-x64 --output ./bin/kora-reclaimer-macos",
    "build:all": "npm run build && pkg . --out-path ./bin"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/sql.js/dist/sql-wasm.wasm",
      "node_modules/sql.js/dist/sql-wasm.js"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "bin"
  },
  "keywords": [
    "solana",
    "kora",
    "rent",
    "reclaim",
    "automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.0",
    "sql.js": "^1.10.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "node-cron": "^3.0.3",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/sql.js": "^1.4.9",
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "@yao-pkg/pkg": "^5.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}